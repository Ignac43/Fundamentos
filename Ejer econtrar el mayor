#include <iostream>
using namespace std;

int main() {
    int a, b, c, mayor;

    cout << "Ingrese el primer numero: ";
    cin >> a;
    cout << "Ingrese el segundo numero: ";
    cin >> b;
    cout << "Ingrese el tercer numero: ";
    cin >> c;

    // Inicializamos mayor con el primero
    mayor = a;

    if (b > mayor)
        mayor = b;
    if (c > mayor)
        mayor = c;

    cout << "El numero mayor es: " << mayor << endl;

    return 0;
}
